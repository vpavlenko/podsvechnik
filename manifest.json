{
  "name": "English syntax highlighter",
  "version": "1.0",
  "description": "Podsvechnik - English syntax highlighter",
  "permissions": ["activeTab", "declarativeContent", "storage", "contextMenus"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/get_started16.png",
    "default_title": "Some tooltip",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
